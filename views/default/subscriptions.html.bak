{{left_sidebar_enabled,right_sidebar_enabled=False,True}}
{{extend 'layout.html'}}
<h1>This is the subscriptions page</h1>
<p>
    This page will list the comics the logged-in user is subscribed to. It will allow a user to unsubscribe from a comic, as well as other administrative functions on their account (there won't be many, as accounts don't have a lot of functions).
</p>

{{block right_sidebar}}
<h6>{{=T("Navigation Sidebar (not fully populated)")}}</h6>
<ul>
  <li>{{=A(T('Browse comics'), _href=URL('browse'))}}</li>
  <li>{{=A(T('Check Updates'), _href=URL('updates'))}}</li>
  <li>{{=A(T('List Subscriptions'), _href=URL('subscriptions'))}}</li>
</ul>
{{end}}
